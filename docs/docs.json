{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Heimdall",
  "colors": {
    "primary": "#3B82F6",
    "light": "#60A5FA",
    "dark": "#1D4ED8"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "icon": "globe",
          "href": "https://tryheimdall.com"
        },
        {
          "anchor": "GitHub",
          "icon": "github",
          "href": "https://github.com/hmdl-inc/heimdall"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "pages": [
          {
            "group": "Getting Started",
            "pages": [
              "guides/introduction",
              "quickstart"
            ]
          },
          {
            "group": "Tracing",
            "pages": [
              "guides/tracing/overview",
              "guides/tracing/python-sdk",
              "guides/tracing/javascript-sdk"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "guides/configuration/environment-variables",
              "guides/configuration/sdk-options"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:founder@tryheimdall.com"
      }
    ],
    "primary": {
      "type": "github",
      "href": "https://github.com/hmdl-inc/heimdall"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/hmdl-inc/heimdall",
      "website": "https://tryheimdall.com"
    }
  }
}
